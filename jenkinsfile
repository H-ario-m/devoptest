pipeline{
    agent any
    tools{
        nodejs 'node'

        
    }
   stages{
    stage('Build'){
        steps{
            sh 'npm install'
            echo 'Build is successful'
        }
   }
   stage('Test'){
    steps{
        sh 'npm test || echo "No tests defined"'
        echo 'Test is successful'
    }
   }
   stage('Deploy'){
    steps{
        sh 'npm run build'
        echo 'Deploy is successful'
    }
   }
   }
}